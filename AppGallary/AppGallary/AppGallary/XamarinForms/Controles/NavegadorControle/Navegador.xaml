<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Padding="10"
             x:Class="AppGallary.XamarinForms.Controles.NavegadorControle.Navegador">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <Label Text="WebView" />
                <Label Text="Propriedades: Source(URL)" />
                <Label Text="Eventos: " />
                <Label Text="Observação: " />
                <Frame BorderColor="Gray" Padding="1">
                    <WebView Source="http://www.google.com" HorizontalOptions="FillAndExpand" HeightRequest="300">
                    
                    </WebView>
                </Frame>
                <Frame BorderColor="Gray" Padding="1">
                    <WebView Source="http://www.youtube.com" HorizontalOptions="FillAndExpand" HeightRequest="300">

                    </WebView>
                </Frame>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>